pre_post_line_graph <- function(category, testgroup) {
category_Exp<-faces_tbl%>%filter(Survey==category, Group==testgroup)
p <- ggplot(category_Exp, aes(x = `Participant #`,y = Total_Score, group = Time)) +
geom_line(aes(color = Time, linetype = Time)) +
geom_point(aes(shape = Time, color = Time)) +
scale_color_manual(values=c("orange", "blue")) +
ggtitle(paste(category," Scores - ",testgroup," Group (Pre vs Post FACES)") +
theme(plot.title = element_text(hjust = 0.5)) #to center title
plot_list <- cbind(plot_list,p)
}
